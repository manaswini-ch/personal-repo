select date_trunc( 'second',end_ts-start_ts) as time_interval from
nabu_ui.data_movement_pre_aggregate_stats
where
pre_aggregate_id =
(select max(pre_aggregate_id) from nabu_ui.data_movement_pre_aggregate_stats
where data_movement_id =$templateData.input_data.data_movement_id$ and batch_id =$first(templateData.get_batch_id).batch_id$ ))
SELECT
CASE WHEN (time_interval IS NOT NULL) THEN (((((((((date_part('Year'::text,time_interval) ||
  ' Years  '::text)||
 (date_part('Mon'::text,time_interval) ||
          ' Months  '::text)||
date_part('day'::text,time_interval)) ||
          ' Days  '::text) ||
date_part('hour', time_interval)) ||
          ' Hours  '::text) ||
 date_part('minute'::text, time_interval)) ||
          ' Mins  '::text) ||
  date_part('second'::text, time_interval)) ||
           ' Secs  '::text)
ELSE NULL::text END AS time
FROM Tablename
>>

get_success_object_count_manual_retry(templateData)::=<<
select distinct(object_id) as count from nabu.flow_task_status fts where batch_id =$first(templateData.get_batch_id).batch_id$ and status_code_id =15 and job_type_id=6
>>

get_failed_object_count_manual_retry(templateData)::=<<
select distinct(object_id) as count from nabu.flow_task_status fts where batch_id =$first(templateData.get_batch_id).batch_id$ and status_code_id =16 and job_type_id=6
>>

list_of_failed_tables_failure_manual_retry(templateData)::=<<
select dataplace_name,
count(status_code_id) as total,
count(case when job_type_id = 6 then 1 end) as retried_count,
        count(case when status_code_id = 15 and job_type_id = 6 then 1 end)  as success_count,
        count(case when status_code_id in (16,17) and job_type_id = 6  then 1 end) as failed_count,
		count(case when status_code_id in (18) and job_type_id = 6  then 1 end) as timed_out
from (
select a.batch_id,c.dataplace_name ,status_code_id ,job_type_id ,row_number()over(partition by batch_id,object_id order by flow_task_ts desc ) as r
from
nabu.flow_task_status a
inner join nabu.dataplace_table_metadata_physical b on a.object_id = b.table_id and b.valid_to_ts = '9999-12-31'
inner join nabu.dataplace_physical c on b.dataplace_id = c.dataplace_id and c.valid_to_ts  = '9999-12-31'
where a.batch_id = $first(templateData.get_batch_id).batch_id$
)a where r = 1
group by dataplace_name
>>

get_counts_of_tables_failure_manual_retry(templateData)::=<<
select
count(status_code_id) as total_objects_count,
count(case when job_type_id = 6 then 1 end) as total_retried_count,
        count(case when status_code_id = 15 and job_type_id = 6 then 1 end)  as successful_count,
        count(case when status_code_id in (16,17) and job_type_id = 6  then 1 end) as failed_count,
		count(case when status_code_id in (18) and job_type_id = 6  then 1 end) as timed_out_count
from (
select a.batch_id,status_code_id ,job_type_id ,row_number()over(partition by batch_id,object_id order by flow_task_ts desc ) as r
from
nabu.flow_task_status a
inner join nabu.dataplace_table_metadata_physical b on a.object_id = b.table_id and b.valid_to_ts = '9999-12-31'
where a.batch_id = $first(templateData.get_batch_id).batch_id$
)a where r = 1
>>

